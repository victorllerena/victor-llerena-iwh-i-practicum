doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Custom Object Table
        a(href='/update-cobj') Add/Update this table 
        table
            thead 
                tr 
                    th Name 
                    th Genre 
                    th Platform 
            tbody
                each object in data
                    tr
                        td #{object.properties.name}
                        td #{object.properties.genres}
                        td #{object.properties.platforms}